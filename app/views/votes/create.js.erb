<% if @vote.new_record? %>
  alert("すでに<%= @type %>しました");
<% else %>
  if($("#<%= @type %>")[0]){
    $("#<%= @type %>").html('<%= j(render :partial=>"thing", locals: {thing: @thing, type: @type}) %>');
  }
  if($("#<%= @type %>-<%= @thing.id %>")[0]){
    $("#<%= @type %>-<%= @thing.id %>").html('<%= j(render :partial=>"icons", locals: {thing: @thing, type: @type}) %>');
  }
<% end %>
